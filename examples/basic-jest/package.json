{
  "name": "tia-jest-example",
  "version": "1.0.0",
  "description": "Node.js server example using TIA with Jest for precise test impact analysis",
  "main": "src/server.ts",
  "scripts": {
    "start": "ts-node src/server.ts",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch", 
    "tia": "tia",
    "tia:line-analysis": "tia line-analysis",
    "tia:affected-tests": "tia affected-tests --format specs"
  },
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "@types/express": "^4.17.17",
    "@types/supertest": "^2.0.12",
    "supertest": "^6.3.3",
    "typescript": "^5.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.1"
  }
}
