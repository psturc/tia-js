{
  "name": "tia-cypress-example",
  "version": "1.0.0",
  "description": "Example project using TIA with Cypress E2E tests",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "dev": "http-server src -p 3000",
    "dev:coverage": "npm run build:coverage && http-server dist -p 3000",
    "build": "webpack --config webpack.config.js",
    "build:coverage": "webpack --config webpack.coverage.config.js",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:coverage": "start-server-and-test dev:coverage 3000 cy:run",
    "test": "start-server-and-test dev 3000 cy:run",
    "test:coverage": "npm run cy:run:coverage",
    "tia": "tia",
    "tia:analyze": "tia analyze",
    "tia:run": "tia run",
    "tia:watch": "tia watch"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "@babel/preset-react": "^7.27.1",
    "@cypress/code-coverage": "^3.12.8",
    "babel-loader": "^9.1.3",
    "babel-plugin-istanbul": "^6.1.1",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "cypress": "^13.0.0",
    "html-webpack-plugin": "^5.5.3",
    "http-server": "^14.1.0",
    "jsdom": "^27.0.0",
    "nyc": "^15.1.0",
    "start-server-and-test": "^2.0.0",
    "style-loader": "^3.3.3",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^5.9.0"
  }
}
